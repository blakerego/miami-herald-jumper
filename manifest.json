{
  "manifest_version": 2,

  "name": "Miami Herald Jumper",
  "description": "Removes paywall modal.",
  "version": "1.0.0",
  "browser_action": {
    "name": "Miami Herald Jumper"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts" : [
    {
      "matches" : [
        "http://*/*"
      ],
      "js" : ["contentscript.js"]
    }
  ]
}